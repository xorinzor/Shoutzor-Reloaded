<html>
    <head>
		<title>{$TITLE}</title>
		{literal}
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<meta name="viewport" content="width=320, target-densitydpi=device-dpi">
		<style type="text/css">
			@media only screen and max-width 660px {
			table[class=w0],td[class=w0]{width:0!important;}
			table[class=w15],td[class=w15],img[class=w15]{width:5px!important;}
			table[class=w130],td[class=w130],img[class=w130]{width:55px!important;}
			table[class=w140],td[class=w140],img[class=w140]{width:90px!important;}
			table[class=w170],td[class=w170],img[class=w170]{width:100px!important;}
			table[class=w255],td[class=w255],img[class=w255]{width:185px!important;}
			table[class=w325],td[class=w325],img[class=w325]{width:95px!important;}
			table[class=w470],td[class=w470],img[class=w470]{width:200px!important;}
			table[class=w580],td[class=w580],img[class=w580]{width:280px!important;}
			table[class=w640],td[class=w640],img[class=w640]{width:300px!important;}
			table[class*=hide],td[class*=hide],img[class*=hide],p[class*=hide],span[class*=hide]{display:none!important;}
			table[class=h0],td[class=h0]{height:0!important;}
			p[class=footer-content-left]{text-align:center!important;}
			table[class=w10],td[class=w10],img[class=w10],table[class=w30],td[class=w30],img[class=w30],table[class=w60],td[class=w60],img[class=w60]{width:10px!important;}
			table[class=w125],td[class=w125],img[class=w125],table[class=w180],td[class=w180],img[class=w180],table[class=w195],td[class=w195],img[class=w195],table[class=w220],td[class=w220],img[class=w220]{width:80px!important;}
			table[class=w160],td[class=w160],img[class=w160],table[class=w240],td[class=w240],img[class=w240],table[class=w410],td[class=w410],img[class=w410]{width:180px!important;}
			table[class=w275],td[class=w275],img[class=w275],table[class=w280],td[class=w280],img[class=w280]{width:135px!important;}
			table[class=w300],td[class=w300],img[class=w300],table[class=w360],td[class=w360],img[class=w360]{width:140px!important;}
			}
			#outlook a{padding:0;}
			body{width:100%!important;background-color:#dedede;margin:0;padding:0;}
			.ReadMsgBody{width:100%;}
			.ExternalClass{width:100%;display:block!important;}
			img{outline:none;text-decoration:none;display:block;}
			br,strong br,b br,em br,i br{line-height:100%;}
			h1,h2,h3,h4,h5,h6{line-height:100%!important;-webkit-font-smoothing:antialiased;}
			h1 a,h2 a,h3 a,h4 a,h5 a,h6 a{color:blue!important;}
			h1 a:active,h2 a:active,h3 a:active,h4 a:active,h5 a:active,h6 a:active{color:red!important;}
			h1 a:visited,h2 a:visited,h3 a:visited,h4 a:visited,h5 a:visited,h6 a:visited{color:purple!important;}
			table td,table tr{border-collapse:collapse;}
			.yshortcuts,.yshortcuts a,.yshortcuts a:link,.yshortcuts a:visited,.yshortcuts a:hover,.yshortcuts a span{color:#000;text-decoration:none!important;border-bottom:none!important;background:none!important;}
			code{white-space:normal;word-break:break-all;}
			#background-table{background-color:#dedede;}
			#top-bar{border-radius:6px 6px 0 0;-moz-border-radius:6px 6px 0 0;-webkit-border-radius:6px 6px 0 0;-webkit-font-smoothing:antialiased;background-color:#adabad;color:#f7f7f7;}
			#top-bar a{font-weight:700;color:#7a7a7a;text-decoration:none;}
			#header{border-radius:6px 6px 0 0;-moz-border-radius:6px 6px 0 0;-webkit-border-radius:6px 6px 0 0;}
			#footer{border-radius:0 0 6px 6px;-moz-border-radius:0 0 6px 6px;-webkit-border-radius:0 0 6px 6px;-webkit-font-smoothing:antialiased;background-color:#c7c7c7;color:#828282;}
			body,td{font-family:'Helvetica Neue', Arial, Helvetica, Geneva, sans-serif;}
			.article-title{font-size:18px;line-height:24px;color:#b0b0b0;font-weight:700;margin-top:0;margin-bottom:18px;font-family:'Helvetica Neue', Arial, Helvetica, Geneva, sans-serif;}
			.article-title a{color:#190940;text-decoration:none;}
			.article-content{font-size:13px;line-height:18px;color:#444;margin-top:0;margin-bottom:18px;font-family:'Helvetica Neue', Arial, Helvetica, Geneva, sans-serif;}
			.article-content a{color:#190940;font-weight:700;text-decoration:none;}
			.article-content p{margin-bottom:15px;}
			.footer-content{font-size:12px;line-height:15px;color:#828282;margin-top:0;margin-bottom:15px;}
			.footer-content a{color:#8a198a;font-weight:700;text-decoration:none;}
			#footer a{color:#8a198a;text-decoration:none;font-weight:700;}
			#permission-reminder{white-space:normal;}
			#street-address{color:#b0b0b0;white-space:normal;}
		</style>
	</head>
	{/literal}
	<body>
		<table width="100%" cellpadding="0" cellspacing="0" border="0" id="background-table">
			<tbody>
				<tr>
					<td align="center" bgcolor="#dedede">
						<table class="w640" style="margin:0 10px;" width="640" cellpadding="0" cellspacing="0" border="0">
							<tbody>
								<tr>
									<td class="w640" width="640" height="20"></td>
								</tr>
								<!-- header -->
								<tr>
									<td id="header" class="w640" width="640" height="100" align="center" bgcolor="#331E65">
										<div align="center" style="text-align: center">
											<a href="{$SITEURL}">
											<img id="customHeaderImage" align="top" label="Image" src="data:image/png;base64," alt="logo" class="w640" border="0" style="display: inline">
											</a>
										</div>
									</td>
								</tr>
                                
								<!-- content -->
								<tr>
									<td class="w640" width="640" height="30" bgcolor="#ffffff"></td>
								</tr>
								<tr id="simple-content-row">
									<td class="w640" width="640" bgcolor="#ffffff">
										<table class="w640" width="640" cellpadding="0" cellspacing="0" border="0">
											<tbody>
												<tr>
													<td class="w30" width="30"></td>
													<td class="w580" width="580" id="content-column">
														<table class="w580" width="580" cellpadding="0" cellspacing="0" border="0">
															<tbody>
																<tr>
																	<td class="w580" width="580">
																		<p align="left" class="article-title">{$TITLE}</p>
																		<div align="left" class="article-content">
																			{$CONTENT}
																		</div>
																	</td>
																</tr>
																<tr>
																	<td class="w580" width="580" height="10"></td>
																</tr>
															</tbody>
														</table>
													</td>
													<td class="w30" width="30"></td>
												</tr>
											</tbody>
										</table>
									</td>
								</tr>
								<tr>
									<td class="w640" width="640" height="15" bgcolor="#ffffff"></td>
								</tr>
                                
								<!-- Footer -->
								<tr>
									<td class="w640" width="640">
										<table id="footer" class="w640" width="640" cellpadding="0" cellspacing="0" border="0" bgcolor="#c7c7c7">
											<tbody>
												<tr>
													<td colspan="5" class="w580 h0" width="640" height="30"></td>
												</tr>
												<tr>
													<td class="w30" width="30"></td>
													<td class="w580" width="420" valign="top">
														<span class="hide">
															<p id="permission-reminder" class="footer-content"><span>You are receiving this email because you registered yourself on {$SITETITLE}.</span></p>
														</span>
														<p align="left" class="footer-content"></p>
													</td>
													<td class="hide w0" width="60"></td>
													<td class="w30" width="30"></td>
												</tr>
												<tr>
													<td colspan="5" class="w580 h0" width="640" height="15"></td>
												</tr>
											</tbody>
										</table>
									</td>
								</tr>
								<tr>
									<td class="w640" width="640" height="60"></td>
								</tr>
							</tbody>
						</table>
					</td>
				</tr>
			</tbody>
		</table>
	</body>
</html>